{% extends 'back_office/backbase.html.twig' %}

{% block title %}
  Tutorship Sessions
{% endblock %}
{% block pageHeading %}
  <h1>Tutorship </h1>
{% endblock %}

{% block body %}
    <h1>Tutorshipsessions</h1>

    <table class="table">
        <tbody>

            <tr>
                <th>Student</th>
                <td>{{ tutorshipsession.idstudent.nom }}</td>
            </tr>
            <tr>
                <th>Tutor</th>
                <td>{{ tutorshipsession.idtutor.nom }}</td>
            </tr>

            <tr>
                <th>Url</th>
                <td>{{ tutorshipsession.url }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ tutorshipsession.type }}</td>
            </tr>
            <tr>
                <th>Body</th>
                <td>{{ tutorshipsession.body }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ tutorshipsession.date ? tutorshipsession.date|date('Y-m-d H:i') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_tutorshipsessions_index') }}">back to list</a>

    <a href="{{ path('app_tutorshipsessions_edit', {'idsession': tutorshipsession.idsession}) }}">edit</a>

    {{ include('back_office/tutorshipsessions/_delete_form.html.twig') }}
{% endblock %}
